[ignore]
.*/public/.*
.*/lib/.*
.*/build/.*
.*/ios/.*
.*/android/.*
.*/node_modules/react-native/.*

[include]
.*/src/.*

[libs]
flow-typed

[options]
module.name_mapper='^src/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.file_ext=.ios.js
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.otf
module.name_mapper.extension='otf' -> 'empty/object'

[lints]
deprecated-call-syntax=off